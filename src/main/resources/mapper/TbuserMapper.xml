<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0/EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.smspr.mapper.TbuserMapper">

    <!---->
    <select id="detail" parameterType="hashMap" resultType="com.example.smspr.dto.CommonDto$TbuserDetailResDto">
        SELECT tbuser.id
             ,tbuser.name
             ,tbuser.phone
             ,tbuser.gender
             ,	,tbuser.title as title2
        FROM tbuser
        WHERE tbuser.id = #{id}
    </select>


</mapper>